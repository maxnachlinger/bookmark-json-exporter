{
  "name": "Bookmark JSON Exporter",
  "version": "1.0.0",
  "description": "A bookmark JSON exporter.",
  "permissions": [
    "bookmarks",
    "downloads"
  ],
  "icons": {
    "16": "16x16.png",
    "48": "48x48.png",
    "128": "128x128.png"
  },
  "browser_action": {
    "default_title": "Bookmark JSON Exporter",
    "default_icon": "16x16.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2
}
